			rtu1_0: rtu@b104000 {
				compatible = "ti,am654-rtu";
				reg = <0xb104000 0x2000>,
				      <0xb123000 0x100>,
				      <0xb123400 0x100>;
				reg-names = "iram", "control", "debug";
				firmware-name = "am65x-rtu1_0-fw";
				interrupt-parent = <&icssg1_intc>;
				interrupts = <20>, <21>;
				interrupt-names = "vring", "kick";
			};

